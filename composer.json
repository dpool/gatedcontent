{
  "name": "dpool/gatedcontent",
  "type": "typo3-cms-extension",
  "description": "Gatedcontent is lead-capture extension for TYPO3. The extension provides editors with a configurable form element to collect user data. In exchange for personal information like e-mail address, name, company etc., visitors can access downloads or a single TYPO3 page.",
  "homepage": "https://www.github.com/dpool/gatedcontent/",
  "license": [
    "GPL-2.0-or-later"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "support": {
    "issues": "https://www.github.com/dpool/gatedcontent/issues"
  },
  "require": {
    "ext-json": "*",
    "typo3/cms-core": "^10.4",
    "typo3/cms-extbase": "^10.4",
    "lcobucci/jwt": "^3.4"
  },
  "config": {
    "platform": {
      "php": "7.2.0"
    }
  },
  "require-dev": {
    "typo3/cms-fluid-styled-content": "^10.4",
    "typo3/cms-rte-ckeditor": "^10.4",
    "typo3/testing-framework": "^6.5",
    "codeception/codeception": "^4.1",
    "codeception/module-webdriver": "^1.0",
    "codeception/module-asserts": "1.3.1",
    "codeception/module-db": "^1.0",
    "helhum/typo3-console": "v6.4.0",
    "helhum/dotenv-connector": "^2.3",
    "ericmartel/codeception-email-mailhog": "^1.0",
    "phpstan/phpstan": "0.12.68",
    "squizlabs/php_codesniffer": "3.5.8",
    "hoa/console": "^3.17"
  },
  "autoload": {
    "psr-4": {
      "Dpool\\Gatedcontent\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dpool\\Gatedcontent\\Tests\\": "Tests/"
    }
  },
  "replace": {
    "dpool/gatedcontent": "self.version",
    "typo3-ter/gatedcontent": "self.version"
  },
  "scripts": {
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "gatedcontent",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
